<div class="post archive">
  <h3>Posts tagged #{{tag}}</h3>
  <table class="archive">
  <tbody>
  {{#each posts}}
    <tr>
    {{#if published}}
    <td>{{ post_index }}<td><a href="/posts/{{ date }}/{{ file }}.html">{{ title }}</a></td><td>{{ display_date }}</td>
    {{/if}}
    </tr>
  {{/each}}
  </tbody>
  </table>
</div>
